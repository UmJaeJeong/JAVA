<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.*?>

<HBox prefHeight="43.0" prefWidth="325.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<!-- Hbox 컨테이너 설정 시작 태그 -->

	<!-- Ctrl + space를 누르면 intelligence 기능이 나온다. 속성이나 컨트롤 컨테이너 등 주석 처리하고자 하는 
		곳을 선택하여 컨트롤 + 쉬프트 + /를 누르면 자동 주석처리가 된다. -->
	<padding>
		<Insets topRightBottomLeft="10" />
		<!-- <Insets bottom = "10" left = "10" right = "10" top = "10"/> -->
	</padding>

	<spacing>10</spacing><!-- 컨트롤간의 간격 설정 -->
	<!-- 내용(컨트롤 추가) -->
	<children>
		<TextField>		<!-- 텍스트 필드 컨트롤 추가 -->
			<prefWidth>200</prefWidth>
		</TextField>

		<!-- <Button text = "확인"/> -->
		<Button>
			<text>확인</text>
		</Button>
      <Button mnemonicParsing="false" text="취소" />
	</children>
</HBox>
